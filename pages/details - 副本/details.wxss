/* pages/details/details.wxss */
::-webkit-scrollbar{
width: 0;
height: 0;
color: transparent;
}
page{width:100%;height:100%;background: #000;position: relative;}
.scoll-h{box-sizing:border-box;height:100%;white-space: nowrap;}
.shadow{position: absolute;top: 0;left: 0;width: 100%;height: 100%;background: url(data:image/png;base64,) no-repeat;z-index: 999;background-size:cover;}
.notice{position:absolute;height:100rpx;background:#e60012;font-size:34rpx;color:#fff;border-radius:10px;top:80rpx;text-align:center;padding:0rpx 36rpx;font-weight: bold;line-height:100rpx;left:50%;margin-left:-150rpx;z-index:100000}
.notice text{font-size:60rpx;}
.notice image{width:85rpx;position:absolute;margin-top:-40rpx;}
.footposi{height:290rpx;position:fixed;bottom:0rpx;padding:30rpx 20rpx 0rpx 20rpx;box-sizing:border-box;z-index:100000;width: 100%;}
.foottitle{font-size:34rpx;color:#ffffff;white-space:pre-wrap;}
.foottext{font-size:24rpx;color:#ffffff;line-height:32rpx;white-space:pre-wrap;}
.footdetail{background:#fff;text-align:center;border-radius:5px;font-size:24rpx;color:#353535;padding:10rpx 20rpx;margin-top:24rpx;position:relative;z-index: 9999;}
.deimg{width:100%;height:100%;position:relative;}
.deimg image{width:100%;position:absolute;top:50%;transform:translateY(-50%);}
.playervideo{width:100%;height:100%;}
.playervideo #player{width:100%;height:100%;}
.stopp{width:100%;height:50%;position:fixed;z-index:1000;top:50%;left:0rpx;text-align:center;transform:translateY(-50%);}
.stopp image{height:100rpx;width:80rpx;position:absolute;top:50%;margin-top:-50rpx;}
.alert{background:rgba(0,0,0,0.3);}
.footad{color: #fff;font-size: 24rpx;vertical-align: middle;}
.footad image{width: 24rpx;height: 28rpx;margin-right: 10rpx;position:relative;top:5rpx;}
.animationSlow{animation:myfirst 6s forwards;
-moz-animation:myfirst 6s forwards;
-webkit-animation:myfirst 6s forwards; 
-o-animation:myfirst 6s forwards;}
@keyframes myfirst {
  0% {
    transform:scale(1.1);
    opacity: 1;
  }
  30%{
    transform:scale(1);
    opacity: 1;
  }
  60%{
    transform:scale(1.1);
    opacity: 1;
  }
  90%{
    transform:scale(1);
    opacity: 1;
  }
  100% {
    transform:scale(1);
    opacity: 0;
    z-index:10;
  }
}
/*兼容ios*/
@-webkit-keyframes myfirst {
  0% {
    transform:scale(1.1);
    opacity: 1;
  }
  30%{
    transform:scale(1);
    opacity: 1;
  }
  60%{
    transform:scale(1.1);
    opacity: 1;
  }
  90%{
    transform:scale(1);
    opacity: 1;
  }
  100% {
    transform:scale(1);
    opacity: 0;
    z-index:10;
  }
}
.hiddenimg{width: 100%;height: 110%;opacity: 0;}
/* .playimg{width: 100%;position: fixed;top: 50%;transform: translateY(-50%);} */
swiper {height: 400rpx;}
swiper-item image {width: 100%;height: 100%;display: block;}
.swiper-container{position: absolute;top: 50%;transform: translateY(-50%);width: 100%;height: 500rpx;}
.swiper-container .swiper{height: 100%;}
.swiper-container .swiper .img{width: 100%;height: 100%;position: absolute;top: 50%;transform: translateY(-50%);}
.foot_btn{height: 120rpx;}
.cover-image{width: 64rpx;height: 80rpx;position: absolute;top: 50%;left: 50%;transform: translate(-50%,-50%);}